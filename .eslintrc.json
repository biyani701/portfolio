{
    "extends": ["plugin:react/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
      "react/prop-types": "error",
        "react/no-deprecated": "warn",
        "no-restricted-syntax": ["off"]
    },
    "settings": {
    "react": {
      "version": "detect"
    }
  }
}